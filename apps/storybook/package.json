{
  "name": "@educacross/storybook",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "dev:debug": "storybook dev -p 6006 --loglevel silly",
    "build": "storybook build",
    "lint": "eslint stories --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf storybook-static",
    "generate:og-image": "node scripts/generate-og-image.mjs",
    "test:storybook": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@fabioeducacross/ui": "workspace:*",
    "@fabioeducacross/ui-education": "workspace:*",
    "lucide-react": "^0.563.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-feather": "^2.0.10",
    "react-zoom-pan-pinch": "^3.7.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "10.2.0",
    "@storybook/addon-coverage": "^3.0.0",
    "@storybook/addon-docs": "10.2.0",
    "@storybook/addon-links": "10.2.0",
    "@storybook/addon-themes": "10.2.0",
    "@storybook/addon-vitest": "10.2.0",
    "@storybook/react": "10.2.0",
    "@storybook/react-vite": "10.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "autoprefixer": "^10.4.20",
    "chromatic": "^13.3.5",
    "eslint-plugin-storybook": "10.2.0",
    "mermaid": "^11.12.2",
    "postcss": "^8.4.49",
    "react-syntax-highlighter": "^16.1.0",
    "rimraf": "^6.0.1",
    "sharp": "^0.34.5",
    "storybook": "10.2.0",
    "tailwindcss": "^3.4.17",
    "ts-dedent": "^2.2.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.6",
    "vitest": "^4.0.18"
  }
}
