{
    "name": "@fabioeducacross/ui-pdf",
    "version": "0.1.0",
    "description": "Educacross Design System - PDF Generation Components",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "engines": {
        "node": ">=20.0.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://npm.pkg.github.com"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "lint": "eslint src --ext .ts,.tsx",
        "typecheck": "tsc --noEmit",
        "clean": "rimraf dist",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage"
    },
    "peerDependencies": {
        "@fabioeducacross/ui": "workspace:*",
        "react": "^18.3.0",
        "react-dom": "^18.3.0",
        "jspdf": "^2.5.2",
        "html2canvas": "^1.4.1"
    },
    "devDependencies": {
        "@fabioeducacross/ui": "workspace:*",
        "@types/react": "^18.3.17",
        "@types/react-dom": "^18.3.5",
        "@typescript-eslint/eslint-plugin": "^8.19.1",
        "@typescript-eslint/parser": "^8.19.1",
        "@vitest/coverage-v8": "^4.0.18",
        "eslint": "^9.18.0",
        "html2canvas": "^1.4.1",
        "jspdf": "^2.5.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "rimraf": "^6.0.1",
        "tsup": "^8.3.5",
        "typescript": "^5.7.2",
        "vitest": "^4.0.18"
    },
    "keywords": [
        "educacross",
        "design-system",
        "react",
        "components",
        "pdf",
        "jspdf",
        "html2canvas",
        "certificates",
        "reports"
    ],
    "author": "Educacross",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/fabioeducacross/Design_System_Educacross",
        "directory": "packages/ui-pdf"
    }
}
